{
  "manifest_version": 3,
  "name": "FlashTunnel",
  "version": "1.0",
  "description": "FlashTunnel Extension",
  "permissions": [
    "proxy",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "FlashTunnel"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://127.0.0.1/*"
    ]
  }
}
