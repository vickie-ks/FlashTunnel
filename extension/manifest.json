{
  "manifest_version": 3,
  "name": "FlashTunnel",
  "version": "1.0",
  "description": "Easily toggle FlashTunnel for your browser.",
  "permissions": [
    "proxy",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}